while 1 > 0 :
    first_number=input("first number: ")
    first_number=float(first_number)
    Computing=input("Computing (* / - +)")
    secend_number=input("secend number :")
    secend_number=float(secend_number)
    if Computing==("*") :
        print(first_number * secend_number)
    if Computing==("+") :
        print(first_number + secend_number)
    if Computing==("/") :
        print(first_number / secend_number)
    if Computing==("-") :
        print(first_number - secend_number)
